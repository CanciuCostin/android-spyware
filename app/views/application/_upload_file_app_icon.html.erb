<span tooltip="OS SUPPORT: <= 10

              REQUISITE&nbsp;&nbsp;&nbsp;&nbsp;: ADB/MSF

              DESCRIPTION: Upload a file on the device" flow="up">
<%= link_to '../smartphones/upload_file?smartphone_id=' + smartphone.id.to_s,
 method: :get, :remote => true , :class => "adb_link", :id => "upload_file_link" do %>
  <%= image_tag "upload_file.png", :alt => "Upload File", :class => "smartphone-icon adb_proc" %>
<% end %>
</span>

<script>
    $("#upload_file_link").on('click',function(event){
        $( ".Terminal__body" ).trigger("write",[" > ", "upload", ["Starting . . ."] ]);
    });
</script>